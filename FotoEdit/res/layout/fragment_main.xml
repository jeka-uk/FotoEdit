<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_2"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.fotoedit.MainActivity$PlaceholderFragment" >

    <ImageView
        android:id="@+id/next"
        android:layout_width="@dimen/imageView_next_layout_width"
        android:layout_height="@dimen/imageView_next_layout_height"
        android:layout_marginLeft="@dimen/imageView_next_marginLeft"
        android:layout_marginTop="@dimen/imageView_next_marginTop"
        android:src="@drawable/next"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/imageViewaAnimCamera"
        android:layout_width="@dimen/imageViewAnimStart_layout_width"
        android:layout_height="@dimen/imageViewAnimStart_layout_height"
        android:layout_marginLeft="@dimen/imageViewAnimStart_marginLeft"
        android:layout_marginTop="@dimen/imageViewAnimStart_marginTop"
        android:src="@drawable/composer_camera" />

    <ImageView
        android:id="@+id/imageViewaAnimGalereya"
        android:layout_width="@dimen/imageViewAnimStart_layout_width"
        android:layout_height="@dimen/imageViewAnimStart_layout_height"
        android:layout_marginLeft="@dimen/imageViewAnimStart_marginLeft"
        android:layout_marginTop="@dimen/imageViewAnimStart_marginTop"
        android:src="@drawable/composer_galereya" />

    <ImageView
        android:id="@+id/imageViewaAnimInternet"
        android:layout_width="@dimen/imageViewAnimStart_layout_width"
        android:layout_height="@dimen/imageViewAnimStart_layout_height"
        android:layout_marginLeft="@dimen/imageViewAnimStart_marginLeft"
        android:layout_marginTop="@dimen/imageViewAnimStart_marginTop"
        android:src="@drawable/composer_internet" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="@dimen/ramka_1_layout_width"
        android:layout_height="@dimen/ramka_1_layout_height"
        android:layout_marginLeft="@dimen/ramka_1_marginLeft"
        android:layout_marginTop="@dimen/ramka_1_marginTop"
        android:rotation="-10"
        android:src="@drawable/ramka" />

    <ImageView
        android:id="@+id/imagetop"
        android:layout_width="@dimen/image_1_layout_width"
        android:layout_height="@dimen/image_1_layout_height"
        android:layout_marginLeft="@dimen/image_1_marginLeft"
        android:layout_marginTop="@dimen/image_1_marginTop"
        android:rotation="-10" />

    <ImageView
        android:id="@+id/imagecamera"
        android:layout_width="@dimen/imageViewAnimStart_1_layout_width"
        android:layout_height="@dimen/imageViewAnimStart_1_layout_height"
        android:layout_marginLeft="@dimen/imageViewAnimStart_1_marginLeft"
        android:layout_marginTop="@dimen/imageViewAnimStart_1_marginTop"
        android:src="@drawable/composer_camera" />

    <ImageView
        android:id="@+id/imagegalerey"
        android:layout_width="@dimen/imageViewAnimStart_1_layout_width"
        android:layout_height="@dimen/imageViewAnimStart_1_layout_height"
        android:layout_marginLeft="@dimen/imageViewAnimStart_1_marginLeft"
        android:layout_marginTop="@dimen/imageViewAnimStart_1_marginTop"
        android:src="@drawable/composer_galereya" />

    <ImageView
        android:id="@+id/imageInternet"
        android:layout_width="@dimen/imageViewAnimStart_1_layout_width"
        android:layout_height="@dimen/imageViewAnimStart_1_layout_height"
        android:layout_marginLeft="@dimen/imageViewAnimStart_1_marginLeft"
        android:layout_marginTop="@dimen/imageViewAnimStart_1_marginTop"
        android:src="@drawable/composer_internet" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/ramka_2_layout_width"
        android:layout_height="@dimen/ramka_2_layout_height"
        android:layout_marginLeft="@dimen/ramka_2_marginLeft"
        android:layout_marginTop="@dimen/ramka_2_marginTop"
        android:rotation="10"
        android:src="@drawable/ramka" />

    <ImageView
        android:id="@+id/imagebuttom"
        android:layout_width="@dimen/image_2_layout_width"
        android:layout_height="@dimen/image_2_layout_height"
        android:layout_marginLeft="@dimen/image_2_marginLeft"
        android:layout_marginTop="@dimen/image_2_marginTop"
        android:rotation="10" />

</RelativeLayout>